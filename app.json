{
  "expo": {

    "scheme": "your-app-scheme",
    "name": "lesapp",
    "slug": "lesapp",
    "version": "1.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ]
    ],
    "permissions": [
      "LOCATION"
    ]
  }

}
